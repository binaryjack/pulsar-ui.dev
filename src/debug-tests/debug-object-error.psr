// Minimal reproduction for object literal parsing error
import { useState } from '@pulsar-framework/pulsar.dev';

component MinimalObjectError() {
  const [filterActive, setFilterActive] = useState<boolean>(false);
  
  return (
    <button
      onClick={() => setFilterActive(!filterActive())}
      style={`background: ${filterActive() ? '#f56565' : '#4299e1'};`}
    >
      Test
    </button>
  );
}