// Test exact JSX mapping pattern from the complex file
import { useState } from '@pulsar-framework/pulsar.dev';

component TestJSXMapping() {
  const [items, setItems] = useState<string[]>(['Item 1', 'Item 2']);
  
  return (
    <div>
      {items().map((item, index) => (
        <div
          key={index}
          style="padding: 1rem; background: #f7fafc;"
