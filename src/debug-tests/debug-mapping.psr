// Test JSX mapping with key attribute 
import { useState } from '@pulsar-framework/pulsar.dev';

component TestMapping() {
  const [items, setItems] = useState<string[]>(['Item 1', 'Item 2']);
  
  return (
    <div>
      {items().map((item, index) => (
        <div key={index}>
          <span>{item}</span>
        </div>
      ))}
    </div>
  );
}