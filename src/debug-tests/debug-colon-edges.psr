// Test edge cases around object property colon parsing
import { useState } from '@pulsar-framework/pulsar.dev';

component TestColonEdgeCases() {
  // Test various property patterns that could confuse the colon parser
  const obj1 = { normal: 'value' };
  const obj2 = { 'string-key': 'value' };
  const obj3 = { component: 'test' }; // Previously problematic keyword
  
  // Test with embedded expressions that might interfere
  const obj4 = {
    count: items().length,
    active: filterActive() ? 'yes' : 'no'
  };
  
  return (
    <div>
      {JSON.stringify(obj1)}
      {JSON.stringify(obj2)}
      {JSON.stringify(obj3)}
      {JSON.stringify(obj4)}
    </div>
  );
}